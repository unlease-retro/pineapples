{
  "name": "pineapples",
  "version": "1.0.0",
  "description": "Pineapples as a Service",
  "main": "server",
  "engines": {
    "node": "~6.3.1"
  },
  "pre-commit": {
    "run": [
      "lint"
    ],
    "silent": true
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "env": {
      "production": {
        "presets": [
          "es2015",
          "react",
          "react-optimize",
          "stage-2"
        ]
      }
    }
  },
  "scripts": {
    "lint": "eslint --config config/.eslintrc.js app/ server/",
    "start": "per-env",
    "start:development": "node scripts/start & NODE_ENV=development nodemon -w server -w public  --exec 'npm run lint && node server'",
    "start:production": "node index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config config/webpack.config.prod.js --progress --profile --colors",
    "build:server": "webpack --config config/webpack.config.server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlease-development/pineapples.git"
  },
  "keywords": [
    "pineapple"
  ],
  "author": "Unlease <hello@unlease.io> (https://www.unlease.io/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unlease-development/pineapples/issues"
  },
  "homepage": "https://github.com/unlease-development/pineapples#readme",
  "dependencies": {
    "aphrodite": "^0.5.0",
    "body-parser": "^1.15.2",
    "configstore": "^2.0.0",
    "connect-redis": "^3.1.0",
    "cors": "^2.7.1",
    "deep-equal": "^1.0.1",
    "emailjs": "^1.0.5",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "express-session": "^1.14.0",
    "immutable": "^3.8.1",
    "mongoose": "^4.5.8",
    "moniker": "^0.1.2",
    "morgan": "^1.7.0",
    "normalize.css": "^4.2.0",
    "ngeohash": "^0.6.0",
    "passwordless": "^1.1.1",
    "passwordless-redisstore": "^1.0.2",
    "per-env": "^1.0.2",
    "postmark": "^1.2.1",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-select": "^1.0.0-beta14",
    "redux": "^3.5.2",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "^2.6.1",
    "reselect": "^2.5.3",
    "stripe": "^4.9.1",
    "turf-centroid": "^3.0.12",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.13.0",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.2.2",
    "eslint-config-unlease": "^3.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-react": "^6.0.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-notifier": "^4.6.1",
    "nodemon": "^1.10.0",
    "pre-commit": "^1.1.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
