{
  "name": "pineapples",
  "version": "1.0.0",
  "description": "Pineapples as a Service",
  "main": "server",
  "engines": {
    "node": "~6.3.1"
  },
  "pre-commit": {
    "run": [
      "lint"
    ],
    "silent": true
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "env": {
      "production": {
        "presets": [
          "es2015",
          "react",
          "react-optimize",
          "stage-2"
        ]
      }
    }
  },
  "scripts": {
    "lint": "eslint --config config/.eslintrc.js app/",
    "start": "per-env",
    "start:development": "webpack --watch --config config/webpack.config.dev.js & NODE_ENV=development nodemon server -w server -w public",
    "start:production": "node server",
    "build": "NODE_ENV=production webpack --config config/webpack.config.prod.js --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlease-development/pineapples.git"
  },
  "keywords": [
    "pineapple"
  ],
  "author": "Unlease <hello@unlease.io> (https://www.unlease.io/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unlease-development/pineapples/issues"
  },
  "homepage": "https://github.com/unlease-development/pineapples#readme",
  "dependencies": {
    "aphrodite": "^0.5.0",
    "body-parser": "^1.15.2",
    "configstore": "^2.0.0",
    "connect-redis": "^3.1.0",
    "cors": "^2.7.1",
    "emailjs": "^1.0.5",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.14.0",
    "immutable": "^3.8.1",
    "mongoose": "^4.5.8",
    "passwordless": "^1.1.1",
    "passwordless-redisstore": "^1.0.2",
    "per-env": "^1.0.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "^2.6.1",
    "reselect": "^2.5.3",
    "serve-favicon": "^2.3.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.13.0",
    "clean-webpack-plugin": "^0.1.10",
    "eslint": "^3.2.2",
    "eslint-config-unlease": "^3.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "nodemon": "^1.10.0",
    "pre-commit": "^1.1.3",
    "webpack": "^1.13.1"
  }
}
